{
  "name": "vue3-skeleton",
  "description": "An elegan skeleton library compatible with Vue 3",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "Brayam Valero",
  "keywords": [
    "vue",
    "vue3",
    "skeleton",
    "component",
    "library"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.4.21",
    "vue-tsc": "^1.8.27"
  },
  "main": "./dist/vue3-skeleton.umd.js",
  "module": "./dist/vue3-skeleton.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-skeleton.esm.js",
      "require": "./dist/vue3-skeleton.js"
    }
  },
  "files": [
    "dist",
    "src"
  ]
}
